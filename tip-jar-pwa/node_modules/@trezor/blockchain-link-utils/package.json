{
  "name": "@trezor/blockchain-link-utils",
  "version": "1.5.1",
  "license": "See LICENSE.md in repo root",
  "sideEffects": false,
  "main": "lib/index.js",
  "publishConfig": {
    "main": "lib/index.js"
  },
  "files": [
    "lib/",
    "!**/*.map"
  ],
  "scripts": {
    "test:unit": "yarn g:jest -c ../../jest.config.base.js",
    "depcheck": "yarn g:depcheck",
    "type-check": "yarn g:tsc --build",
    "build:lib": "yarn g:rimraf ./lib && yarn g:tsc --build tsconfig.lib.json && ../../scripts/publish/replace-imports.sh ./lib cjs",
    "prepublishOnly": "yarn tsx ../../scripts/publish/prepublishNPM.js",
    "prepublish": "yarn tsx ../../scripts/publish/prepublish.js"
  },
  "dependencies": {
    "@mobily/ts-belt": "^3.13.1",
    "@stellar/stellar-sdk": "14.2.0",
    "@trezor/env-utils": "1.5.0",
    "@trezor/protobuf": "1.5.1",
    "@trezor/utils": "9.5.0",
    "xrpl": "4.4.3"
  },
  "devDependencies": {
    "@solana/addresses": "^2.3.0",
    "@solana/keys": "^2.3.0",
    "@solana/rpc-api": "^2.3.0",
    "@solana/rpc-types": "^2.3.0",
    "@trezor/blockchain-link-types": "1.5.0",
    "@trezor/eslint": "1.0.0",
    "@trezor/type-utils": "1.2.0",
    "tsx": "^4.20.3"
  },
  "peerDependencies": {
    "tslib": "^2.6.2"
  }
}